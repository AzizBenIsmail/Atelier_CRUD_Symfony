{{ form(searchStudent) }}
<table border="1">
    <tr>
        <td>NSC</td>
        <td>Email</td>
        <td>Classe</td>
        <td>Action</td>
    </tr>
    {% for student in students %}
        <tr>
            <td>{{ student.ncl }}</td>
            <td>{{ student.Email }}</td>
            <td>{{ student.classroom.Name }}</td>

        </tr>
    {% endfor %}
</table>
